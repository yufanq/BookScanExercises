			<if test="bId != null">
				#{bId,jdbcType=INTEGER},
			</if>
			<if test="bName != null">
				#{bName,jdbcType=VARCHAR},
			</if>
		</trim>
	</insert>
	<update id="updateByPrimaryKeySelective" parameterType="Book">
		update book
		<set>
			<if test="bName != null">
				b_name = #{bName,jdbcType=VARCHAR},
			</if>
		</set>
		where b_id = #{bId,jdbcType=INTEGER}
	</update>
	<update id="updateB" parameterType="Book">
